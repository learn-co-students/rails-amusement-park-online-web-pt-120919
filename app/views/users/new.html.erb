<%= form_for User.new do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label 'Are you an park owner?'%>
  <%= f.check_box :admin %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
   <%= f.label :nausea %>
  <%= f.number_field :nausea %><br>
  <%= f.label :happiness %>
  <%= f.number_field :happiness %><br>
   <%= f.label :tickets %>
  <%= f.number_field :tickets %><br>
  <%= f.label :height %>
  <%= f.number_field :height %><br>
  <%= f.submit %>
<% end %>